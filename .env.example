# Chutes AI (LLM inference)
CHUTES_API_KEY=your_chutes_api_key_here
CHUTES_BASE_URL=https://llm.chutes.ai/v1

# Per-agent model overrides (optional)
PLANNER_MODEL=Qwen/Qwen3-235B-A22B
CODECHECKER_MODEL=Qwen/Qwen2.5-Coder-32B-Instruct
DEBUGGER_MODEL=Qwen/Qwen2.5-Coder-32B-Instruct
TRAINER_MODEL=Qwen/Qwen3-235B-A22B
PUBLISHER_MODEL=Qwen/Qwen2.5-Coder-32B-Instruct

# Default model
DEFAULT_MODEL=Qwen/Qwen2.5-Coder-32B-Instruct

# Basilica GPU cloud (basilica-sdk)
BASILICA_API_TOKEN=your_basilica_api_token_here
BASILICA_API_URL=https://api.basilica.ai
# Budget cap — only rent GPUs at or below this rate (USD/hr)
BASILICA_MAX_HOURLY_RATE=0.44
# Comma-separated GPU type allowlist (substring match, for SSH rentals)
BASILICA_ALLOWED_GPU_TYPES=TESLA V100,RTX-A4000,RTX-A6000
# Deployment-based training (Docker image approach — recommended)
BASILICA_DEPLOY_IMAGE=ghcr.io/tensorlink-ai/synth-city-gpu:latest
# BASILICA_DEPLOY_GPU_MODELS=A4000,A6000
# BASILICA_DEPLOY_MIN_GPU_MEMORY_GB=12
# Pod resource requests (lower = easier to schedule on available nodes)
# BASILICA_DEPLOY_CPU=2000m
# BASILICA_DEPLOY_MEMORY=8Gi

# Bittensor
BT_WALLET_NAME=default
BT_HOTKEY_NAME=default
BT_NETWORK=finney
BT_NETUID=50

# Market data
PYTH_PRICE_FEED_URL=https://hermes.pyth.network

# open-synth-miner research defaults
RESEARCH_N_PATHS=100
RESEARCH_D_MODEL=32
RESEARCH_HORIZON=12
RESEARCH_SEQ_LEN=32
RESEARCH_FEATURE_DIM=4
RESEARCH_BATCH_SIZE=4
RESEARCH_LR=0.001
RESEARCH_EPOCHS=1

# Publishing
HF_TOKEN=your_huggingface_token_here
HF_REPO_ID=your-username/SN50-Hybrid-Hub
WANDB_PROJECT=synth-city
TRACKIO_PROJECT=synth-city

# Hippius decentralised storage (S3-compatible)
# Any S3-compatible endpoint works (Hippius, Cloudflare R2, MinIO, AWS S3)
HIPPIUS_ENDPOINT=https://s3.hippius.com
HIPPIUS_ACCESS_KEY=your_hippius_access_key
HIPPIUS_SECRET_KEY=your_hippius_secret_key
HIPPIUS_BUCKET=synth-city

# Pipeline
MAX_AGENT_TURNS=50
WORKSPACE_DIR=./workspace

# OpenClaw bridge server
BRIDGE_HOST=127.0.0.1
BRIDGE_PORT=8377
# Optional API key for remote access.  When set, every request must include
# an ``X-API-Key`` header with this value.  Leave empty for localhost use.
# BRIDGE_API_KEY=
